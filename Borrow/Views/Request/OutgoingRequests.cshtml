@model UserBorrowRequestsViewModel

<h1>Here are your outgoing Requests</h1>
<div>
    <div>
        <table style="width:100%">
            <tr>
                <th>Item</th>
                <th>Owner</th>
                <th>Term</th>
                <th>Rate</th>
                <th>Return Date</th>
                <th>Requested Created</th>
                <th>Status</th>
                <th></th>
            </tr>
            @for (int i = 0; i < Model.Outgoing.Count(); ++i)
            {
                var item = Model.Outgoing[i];
                <tr>
                    <td>
                        @item.Item
                    </td>
                    <td>
                        @item.OwnerUserName
                    </td>
                    <td>
                        @item.RequestType
                    </td>
                    <td>
                        @item.RequestRate
                    </td>
                    <td>
                        @item.ReturnDate
                    </td>
                    <td>
                        @item.CreatedDateUtc
                    </td>
                    <td>
                        @item.Status
                    </td>
                    <td>THIS WILL BE CANCEL BUTTON</td>
                </tr>
            }
        </table>
    </div>
</div>