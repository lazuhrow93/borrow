@{
    ViewData["Title"] = "Meet up Spot Page";
}



@using Microsoft.AspNetCore.Identity
@inject SignInManager<User> signInManager
@if (signInManager.IsSignedIn(User))
{
    
    <div class="text-center">
        <h1 class="display-4">Lets meet up! </h1>
    </div>
}
else{

    <div class="text-center">
        <h1 class="display-4">Uh oh! Something went wrong. Please sign in again.</h1>
    </div>
}

